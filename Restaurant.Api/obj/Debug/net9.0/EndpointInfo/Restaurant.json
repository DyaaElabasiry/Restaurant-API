{
  "openapi": "3.0.4",
  "info": {
    "title": "Restaurant",
    "version": "1.0"
  },
  "paths": {
    "/api/weather/generate/{count}": {
      "post": {
        "tags": [
          "WeatherForecast"
        ],
        "parameters": [
          {
            "name": "count",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TemperatureRange"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/TemperatureRange"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/TemperatureRange"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "TemperatureRange": {
        "type": "object",
        "properties": {
          "min": {
            "type": "integer",
            "format": "int32"
          },
          "max": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
    }
  }
}